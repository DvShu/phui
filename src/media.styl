@media screen and (max-width: 768px)
  // App
  .site-mobile-shade
    position fixed
    top 0
    right 0
    width 100%
    height 100%
    background-color rgba(0,0,0,.8);
    z-index 101
  .doc-aside
    position fixed
    top 0
    left 0
    z-index 102
    width 220px
  .aside-link
    font-size 14px

  // Home
  .title
    font-size 20px
  .sub-title
    font-size 12px
  .title-note
    font-size 10px
